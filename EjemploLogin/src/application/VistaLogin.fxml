<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="137.0" prefWidth="226.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="application.ControladorLogin">
   <children>
      <GridPane layoutX="14.0" layoutY="14.0" prefHeight="80.0" prefWidth="198.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="txtUsuario" promptText="Usuario" />
            <PasswordField fx:id="txtPassword" promptText="Password" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="87.0" layoutY="100.0" mnemonicParsing="false" onAction="#abrirFormularioPrincipal" text="Ingresar" />
   </children>
</AnchorPane>
