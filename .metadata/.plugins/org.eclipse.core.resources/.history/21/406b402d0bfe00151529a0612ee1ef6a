package application;

import java.net.URL;
import java.util.ResourceBundle;

import javafx.fxml.Initializable;
import utilidades.GestorConexiones;


public class ControladorVistaTableros implements Initializable{
	private GestorConexiones gestorConexion;
	private Main main;
	
	@Override
	public void initialize(URL arg0, ResourceBundle arg1) {
		
	}

	@FXML
	public void mostrarFormularioComentarios(){
		main.mostrarFormularioComentarios();
	}
	
	public Main getMain() {
		return main;
	}

	public void setMain(Main main) {
		this.main = main;
	}	
}
